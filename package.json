{
  "name": "retweetbot",
  "version": "0.0.1",
  "description": "retweets member organizations",
  "private": true,
  "main": "dist/main.js",
  "scripts": {
    "start": "node --enable-source-maps dist/main.js",
    "dev": "NODE_ENV=development ts-node -T src/main.ts -- local",
    "build": "rm -rf dist/ && tsc",
    "test": "run-p -cl test:*",
    "test:ts": "tsc --noEmit",
    "test:eslint": "eslint --color 'src/**/*'",
    "test:prettier": "prettier 'src/**/*' -l",
    "prettier": "prettier 'src/**/*' --write"
  },
  "author": "game workers <https://github.com/gameworkers>",
  "license": "AGPL-3.0",
  "dependencies": {
    "@sentry/integrations": "^6.19.2",
    "@sentry/node": "^6.19.2",
    "async-retry": "^1.3.3",
    "dotenv": "^16.0.0",
    "twitter-api-client": "^1.5.2",
    "znv": "^0.3.1",
    "zod": "^3.14.2"
  },
  "devDependencies": {
    "@types/async-retry": "^1.4.3",
    "@types/node": "^16.11.26",
    "eslint": "^8.12.0",
    "eslint-config-lostfictions": "^3.5.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.1",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "engines": {
    "node": ">=16"
  }
}
